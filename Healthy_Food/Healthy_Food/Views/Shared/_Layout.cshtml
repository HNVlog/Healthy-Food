<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>@ViewBag.Title</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Dry Fruits project">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    @Styles.Render("~/User/css")
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <link href="https://fonts.googleapis.com/css2?family=Josefin+Sans&family=Lora:ital@1&family=Source+Code+Pro:ital,wght@1,500&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.0/font/bootstrap-icons.css">
</head>
<body>
    <div class="super_container">
        @Html.Partial("_Header")

        @RenderBody()

        @Html.Partial("_Footer")

    </div>
    @Scripts.Render("~/User/js")

</body>
</html>
